<body class="hold-transition login-page">
<div class="login-box">
  <div class="login-logo">
    <a href="#""><%= process.env.PROJECT_NAME%></a>
  </div>
  <!-- /.login-logo -->
  <div class="login-box-body">
    <form id="loginAdmin">
      <div class="form-group has-feedback">
        <input type="text" name="email" class="form-control" placeholder="Email">
        <span class="glyphicon glyphicon-envelope form-control-feedback"></span>
      </div>
      <div class="form-group has-feedback">
        <input type="password" name="password" class="form-control" placeholder="Password">
        <span class="glyphicon glyphicon-lock form-control-feedback"></span>
      </div>
      <div class="row col-md-12">
          <div id="errorHolder" class="error" style="display:none;">
              <p style="color: red;"></p>
          </div>
      </div>
      <div class="row">
        <!-- /.col -->
        <div class="col-xs-offset-4 col-xs-4">
          <button type="submit" class="btn btn-primary btn-block btn-flat">
              <i class="fa fa-circle-o-notch fa-spin" style="display:none;"></i>&nbsp;&nbsp;Sign In</button>
        </div>
        <!-- /.col -->
      </div>
    </form>
    <!-- /.social-auth-links -->
  </div>
  <!-- /.login-box-body -->
</div>
<!-- /.login-box -->

<script src="<%= host %>/admin-assets/bower_components/jquery/dist/jquery.min.js"></script>
<!-- Bootstrap 3.3.7 -->
<script src="<%= host %>/admin-assets/bower_components/bootstrap/dist/js/bootstrap.min.js"></script>
<!-- AdminLTE App -->
<script src="<%= host %>/admin-assets/dist/js/adminlte.min.js"></script>
<!-- MainJS -->
<script src="<%= host %>/main.js"></script>

</body>
</html>